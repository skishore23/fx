    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@slack/bolt": "^4.3.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fp-ts": "^2.16.10",
    "inquirer": "^12.6.1",
    "jsforce": "^3.8.1",
