{
  "name": "@fx/framework",
  "version": "1.0.0",
  "description": "Functional programming framework with category theory principles for building AI agents",
  "private": false,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
