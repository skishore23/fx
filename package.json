{
  "name": "q2c-cli",
  "version": "1.0.0",
  "description": "Salesforce Q2C CLI with JWT authentication",
  "main": "dist/cli.js",
  "bin": {
    "q2c": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "q2c": "node dist/cli.js",
    "test": "ts-node src/q2c/test.ts",
    "start:bot": "ts-node src/q2c/slackBot.ts",
    "seed": "ts-node src/q2c/seedData.ts",
    "mock": "ts-node src/q2c/mockSlack.ts",
    "cli": "ts-node src/q2c/run.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/inquirer": "^9.0.8",
